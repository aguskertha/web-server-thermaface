

    <div class="container-fluid">

        <!-- Page Heading -->
        <h2 style="font-family: san-serif;" class="text-left font-weight-bold ml-3">Device</h2>
        <a style="text-decoration:none; color: rgb(102, 99, 99); margin-left: 15px;" href="/dasboard">Home</a> / Device
   
        <!-- DataTales Example -->
        <div class="card shadow mb-4 mt-4" style="padding: 20px;">
          
                <div class="table-responsive">
                    <span>Total Device : 1</span>
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Code</th>
                                <th>Name</th>
                             
                            </tr>
                        </thead>
                        <tfoot>

                            <tr>
                                <td><%= device.deviceID %></td>
                                <td><%= device.deviceCode %></td>
                                <td><%= device.name %></td>
                          
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    <!-- /.container-fluid -->



